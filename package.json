{
  "name": "city-selector-control",
  "version": "0.3.2",
  "description": "A City Selector Control for Mapbox GL JS",
  "files": [
    "dist"
  ],
  "main": "./dist/city-selector-control.es.js",
  "module": "./dist/city-selector-control.umd.js",
  "exports": {
    ".": {
      "import": "./dist/city-selector-control.es.js",
      "require": "./dist/city-selector-control.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "style": "./dist/style.css",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "control",
    "city selector"
  ],
  "repository": "https://github.com/huanglii/city-selector-control.git",
  "author": "黄俐 <li.huangli@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
